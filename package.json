    "conventional-changelog-angular": "^8.1.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.39.1",
    "esno": "^4.8.0",
    "rimraf": "^6.1.2",
    "taze": "^19.9.2"
  }
}
